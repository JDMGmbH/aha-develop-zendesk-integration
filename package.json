{
  "name": "@aha-develop/zendesk",
  "description": "Zendesk integration extension",
  "version": "0.0.0",
  "author": "Aha!",
  "repository": {
    "type": "git",
    "url": "https://github.com/aha-develop/zendesk"
  },
  "dependencies": {},
  "license": "MIT",
  "ahaExtension": {
    "cspSources": [
      "https://*.zendesk.com"
    ],
    "contributes": {
      "importers": {
        "import": {
          "title": "Zendesk tickets",
          "entryPoint": "src/import.js"
        }
      },
      "settings": {
        "subdomain": {
          "title": "Subdomain",
          "scope": [
            "account"
          ],
          "type": "string"
        },
        "dashboardViews": {
          "title": "Dashboard view IDs",
          "scope": [
            "user"
          ],
          "type": "number",
          "array": true
        }
      },
      "views": {
        "tickets": {
          "title": "Zendesk Tickets",
          "entryPoint": "src/tickets.js",
          "host": "page",
          "location": {
            "menu": "Work"
          }
        }
      }
    }
  }
}
